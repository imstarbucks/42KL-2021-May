#include <unistd.h>
#include <stdlib.h>
void	ft_putchar(char c)
{	
	write(1, &c, 1);
}

int	ft_atoi(char *str)
{
	while (*str)
	{
		if (*str >= '0' && <= '9')
		{
			
		}
	}
}

int	main()
{
	int val;
	val = atoi("---+--+1234acb567");
	printf("%d", val);
}
